modules = ["nodejs-20", "postgresql-16"]
[agent]
expertMode = true

[nix]
channel = "stable-25_05"
packages = ["chromium"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "DIY Genie App"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Backend API Server"

[[workflows.workflow]]
name = "DIY Genie App"
author = "agent"

[workflows.workflow.metadata]
outputType = "webview"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npx expo start --web --port 5000"
waitForPort = 5000

[[workflows.workflow]]
name = "Backend API Server"
author = "agent"

[workflows.workflow.metadata]
outputType = "console"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "NODE_ENV=development node server.js"

